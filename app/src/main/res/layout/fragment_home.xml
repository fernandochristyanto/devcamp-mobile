<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:padding="10dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <EditText
        android:id="@+id/etSearch"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:enabled="false"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <View
        android:id="@+id/p1"
        android:layout_width="0dp"
        android:background="@drawable/bg_placeholder"
        android:layout_height="120dp"
        app:layout_constraintTop_toBottomOf="@id/etSearch"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <View
        android:id="@+id/p2"
        android:layout_width="0dp"
        android:background="@drawable/bg_placeholder"
        android:layout_marginTop="30dp"
        android:layout_height="60dp"
        app:layout_constraintTop_toBottomOf="@id/p1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
    <View
        android:id="@+id/p31"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/p2"
        app:layout_constraintEnd_toStartOf="@id/p32"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"/>
    <View
        android:id="@+id/p32"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/p2"
        app:layout_constraintStart_toEndOf="@id/p31"
        app:layout_constraintEnd_toStartOf="@id/p33"
        android:background="@drawable/bg_placeholder"
        android:layout_marginTop="10dp"/>
    <View
        android:id="@+id/p33"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/p2"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintStart_toEndOf="@id/p32"
        app:layout_constraintEnd_toStartOf="@id/p34"
        android:layout_marginTop="10dp"/>
    <View
        android:id="@+id/p34"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/p2"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintStart_toEndOf="@id/p33"
        app:layout_constraintEnd_toStartOf="@id/p35"
        android:layout_marginTop="10dp"/>
    <View
        android:id="@+id/p35"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/p2"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintStart_toEndOf="@id/p34"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="10dp"/>

    <TextView
        android:id="@+id/tvTerlaris"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Terlaris untukmu"
        app:layout_constraintTop_toBottomOf="@id/p31"
        style="@style/TextAppearance.AppCompat.Title"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="20dp"/>
    <TextView
        android:id="@+id/tvLihatTerlaris"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lihat semua"
        android:enabled="false"
        app:layout_constraintBaseline_toBaselineOf="@id/tvTerlaris"
        app:layout_constraintTop_toBottomOf="@id/p31"
        style="@style/TextAppearance.AppCompat.Title"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"/>
    <View
        android:id="@+id/p41"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintTop_toBottomOf="@id/tvTerlaris"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/p42"/>
    <View
        android:id="@+id/p42"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintTop_toBottomOf="@id/tvTerlaris"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toEndOf="@id/p41"
        app:layout_constraintEnd_toStartOf="@id/p43"/>
    <View
        android:id="@+id/p43"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintTop_toBottomOf="@id/tvTerlaris"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toEndOf="@id/p42"
        app:layout_constraintEnd_toEndOf="parent"/>
    <!--<Button-->
        <!--android:id="@+id/btnProducts"-->
        <!--android:layout_width="match_parent"-->
        <!--android:layout_height="match_parent"-->
        <!--android:text="products"/>-->
    <TextView
        android:id="@+id/tvGrgSales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Garage Sales!"
        app:layout_constraintTop_toBottomOf="@id/p41"
        style="@style/TextAppearance.AppCompat.Title"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="20dp"/>
    <TextView
        android:id="@+id/tvLihatGarageSales"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Lihat semua"
        app:layout_constraintBaseline_toBaselineOf="@id/tvGrgSales"
        app:layout_constraintTop_toBottomOf="@id/p41"
        style="@style/TextAppearance.AppCompat.Title"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="20dp"/>
    <View
        android:id="@+id/p51"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintTop_toBottomOf="@id/tvGrgSales"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/p52"/>
    <View
        android:id="@+id/p52"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintTop_toBottomOf="@id/tvGrgSales"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toEndOf="@id/p51"
        app:layout_constraintEnd_toStartOf="@id/p53"/>
    <View
        android:id="@+id/p53"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:background="@drawable/bg_placeholder"
        app:layout_constraintTop_toBottomOf="@id/tvGrgSales"
        android:layout_marginTop="10dp"
        app:layout_constraintStart_toEndOf="@id/p52"
        app:layout_constraintEnd_toEndOf="parent"/>
    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/p53"
        app:layout_constraintBottom_toBottomOf="parent"
        android:background="@drawable/bg_placeholder"
        android:layout_marginTop="20dp" />
</androidx.constraintlayout.widget.ConstraintLayout>